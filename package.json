{
  "name": "isawyou",
  "version": "1.0.0",
  "description": "An app about people who're trying to find each other.",
  "author": "George Katsanos <george@gplus.gr>",
  "private": false,
  "scripts": {
    "preinstall": "npm install -g yarn",
    "precommit": "yarn lint",
    "dev": "node build/dev-server.js",
    "start": "serve -s dist",
    "postinstall": "yarn build:production",
    "build:production": "node build/build.js",
    "test": "jest --coverage",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch"
  },
  "dependencies": {
    "concurrently": "^3.5.0",
    "popper.js": "^1.12.3",
    "vee-validate": "^2.0.0-rc.14",
    "vue": "^2.5.3",
    "vue-router": "3.0.1",
    "vuetify": "^0.16.9",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "axios": "^0.17.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-vue-app": "^1.2.1",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "concurrently": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "node-sass": "^4.6.0",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "serve": "^6.4.1",
    "shelljs": "^0.7.6",
    "url-loader": "^0.5.8",
    "vue-jest": "^1.0.1",
    "vue-loader": "^13.4.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.3",
    "vue-test-utils": "^1.0.0-beta.4",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": "^8.0.0",
    "yarn": "^1.2.1",
    "npm": "^5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "@(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/server"
    ]
  }
}

